<% include nav.ejs %>
<form id="login" method="POST">
    <h1>Log In</h1>
    <fieldset id="inputs">
        <input id="email" name="email" type="email" placeholder="Email" autofocus required>
        <input id="password" name="password" type="password" placeholder="Password" required>
